{
  "order": [
    "TypeKnownBlocks",
    "BlockGpawUnknown",
    "Spacer"
  ],
  "TypeKnownBlocks": {
    "order": [
      "BlockGpawIcon",
      "BlockGpawDipole"
    ],
    "BlockGpawIcon": {
      "p_type": "Block",
      "p_subtype": "BlockGpawIcon",
      "pattern": "^((?:[ \\t]*___ ___ ___ _ _ _[ \\t]*\\n)(?:.*?\\n)*?)(?=[ \\\\t]*$)",
      "flags": [
        "MULTILINE"
      ],
      "comment": "Searching for the first line of gpaw icon, assuming that the icon ends at the first empty line or block pattern."
    },
    "BlockGpawDipole": {
      "p_type": "Block",
      "p_subtype": "BlockGpawDipole",
      "pattern": "^(Dipole moment:.*?\\n)",
      "flags": [
        "MULTILINE"
      ],
      "comment": "Dipole moment"
    }
  },
  "BlockGpawUnknown": {
    "p_type": "Block",
    "p_subtype": "BlockGpawUnknown",
    "pattern": "^((?:(?!^[ \\t]*$).*(?:\\n(?=(?!^[ \\t]*$).*))?)+)\\n",
    "flags": [
      "MULTILINE"
    ],
    "comment": "I have no clue what that is but it is not empty and is not my marker"
  },
  "Spacer": {
    "p_type": "Spacer",
    "p_subtype": "Spacer",
    "pattern": "^(\\s*)\\n",
    "flags": [
      "MULTILINE"
    ],
    "comment": "Just some empty lines, may have tabs and spaces"
  }
}